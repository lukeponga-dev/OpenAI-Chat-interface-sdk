{
  "name": "ai-chat-mobile",
  "version": "1.0.0",
  "description": "AI Chat Mobile Application",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build:static": "next build",
    "vercel-build": "next build",
    "cap:build": "npm run build:static && npx cap copy",
    "cap:sync": "npm run build:static && npx cap sync",
    "cap:android": "npm run cap:build && npx cap open android",
    "cap:ios": "npm run cap:build && npx cap open ios",
    "approve-builds": "pnpm config set ignore-scripts false"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/cli": "latest",
    "@capacitor/core": "latest",
    "@capacitor/haptics": "latest",
    "@capacitor/keyboard": "latest",
    "@capacitor/network": "latest",
    "@capacitor/share": "latest",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/status-bar": "latest",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-select": "latest",
    "@radix-ui/react-tooltip": "^1.0.7",
    "child_process": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fs": "latest",
    "lucide-react": "^0.460.0",
    "next": "15.2.4",
    "next-themes": "latest",
    "path": "latest",
    "react": "^19",
    "react-dom": "^19",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.1.0",
    "postcss": "^8.5.1",
    "prettier": "^3.1.0",
    "sharp": "^0.33.0",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2"
  }
}